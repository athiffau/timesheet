{
  "name": "timesheet",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "upgrade-interactive": "npm-check --update"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "date-fns": "^v2.0.0-alpha.27",
    "vue": "^2.6.9",
    "vue-axios": "^2.1.4",
    "vue-markdown": "^2.2.4",
    "vue-router": "^3.0.2",
    "vuetify": "^1.5.7",
    "vuetify-stylus-fixed-table-header": "^1.1.0",
    "vuetify-upload-button": "^1.2.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-config-vuetify": "^0.2.0",
    "eslint-plugin-vue": "^5.2.2",
    "npm-check": "^5.9.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.9",
    "vuetify-loader": "^1.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "vuetify"
    ],
    "rules": {
      "vue/max-attributes-per-line": [
        "error",
        {
          "singleline": 10,
          "multiline": {
            "max": 5,
            "allowFirstLine": true
          }
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
